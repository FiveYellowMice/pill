[
	{% for pill in site.pills reversed %}{
		"thing": "{{ pill.thing }}",
		"title": "{{ pill.title }}",
		"date": "{{ pill.date | date: "%F" }}",
		"url": "{{ pill.url }}",
		"content": "{{ pill.content | strip_newlines | replace: '"', '\"' }}"
	}{% unless forloop.last %},{% endunless %}{% endfor %}
]
